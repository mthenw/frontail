{
  "name": "frontail",
  "version": "4.9.2",
  "description": "streaming logs to the browser",
  "homepage": "https://github.com/mthenw/frontail",
  "author": "Maciej Winnicki <maciej.winnicki@gmail.com>",
  "license": "MIT",
  "bin": "./bin/frontail",
  "dependencies": {
    "basic-auth-connect": "1.0.0",
    "byline": "5.0.0",
    "CBuffer": "2.2.0",
    "command-exists": "1.2.9",
    "commander": "7.1.0",
    "configstore": "5.0.1",
    "connect": "3.7.0",
    "cookie": "0.4.1",
    "cookie-parser": "1.4.5",
    "daemonize-process": "~3.0.0",
    "express-session": "1.17.1",
    "fs-tail-stream": "1.1.0",
    "is-docker": "2.1.1",
    "serve-static": "1.14.1",
    "socket.io": "3.1.1",
    "universal-analytics": "0.4.23",
    "untildify": "4.0.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "eslint": "~7.20.0",
    "eslint-config-airbnb-base": "~14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "~2.22.1",
    "jsdom": "~16.4.0",
    "mocha": "~8.3.0",
    "pkg": "~4.4.9",
    "should": "~13.2.3",
    "sinon": "~9.2.4",
    "supertest": "^6.1.3",
    "temp": "~0.9.4"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "mocha -r should --exit test/*.js",
    "pkg": "pkg --out-path=dist ."
  },
  "pkg": {
    "assets": [
      "preset/*.json",
      "web/**/*"
    ],
    "targets": [
      "node12-alpine-x64",
      "node12-linux-x64",
      "node12-macos-x64"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "rules": {
      "no-console": "off",
      "strict": "off",
      "implicit-arrow-linebreak": "off"
    },
    "env": {
      "node": true
    },
    "ignorePatterns": [
      "web/assets/tinycon.min.js",
      "web/assets/ansi_up.js",
      "web/assets/darkmode/theme.js"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/mthenw/frontail.git"
  },
  "keywords": [
    "tail",
    "syslog",
    "realtime",
    "log",
    "devops"
  ],
  "main": "index",
  "preferGlobal": true
}
